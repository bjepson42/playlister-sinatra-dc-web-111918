<!DOCTYPE html>
<html>
<head>
	<title>Genre</title>
</head>
<body>
	<h1><%=@genre.name%></h1>
	
	
	<ul>
		<h2>List of artists</h2>
		<% @genre.artists.each do |artist|%>
			<li>
			<a href="/artists/<%= artist.slug %>"><%= artist.name %></a>
			</li>
		<%end%> 
	</ul>
	<ul>
		<h2>List of songs</h2>
		<% @genre.songs.each do |song|%>
			<li>
			<a href="/songs/<%= song.slug %>"><%= song.name %></a>
			</li>
		<%end%>
	</ul>
		

	
</body>
</html>
