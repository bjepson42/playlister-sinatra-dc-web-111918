<!DOCTYPE html>
<html>
<head>
	<title>Artist</title>
</head>
<body>
	<h1><%=@artist.name%></h1>
	
	
	<ul>
		<h2>List of songs</h2>
		<% @artist.songs.each do |song|%>
			<li>
			<a href="/songs/<%= song.slug %>"><%= song.name %></a>
			</li>
		<%end%> 
	</ul>
	<ul>
		<h2>List of genres</h2>
		<% @artist.genres.each do |genre|%>
			<li>
			<a href="/genres/<%= genre.slug %>"><%= genre.name %></a>
			</li>
		<%end%>
	</ul>
		

	
</body>
</html>