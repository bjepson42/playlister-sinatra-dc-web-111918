<!DOCTYPE html>
<html>
<head>
	<title>Song</title>
</head>
<body>
	<h1><%=@song.name%></h1>
	
	
	<ul>
		<h2>List of artists</h2>
		<% @song.artists.each do |artist|%>
			<li>
			<a href="/artists/<%= artist.slug %>"><%= artist.name %></a>
			</li>
		<%end%> 
	</ul>
	<ul>
		<h2>List of genres</h2>
		<% @song.genres.each do |genre|%>
			<li>
			<a href="/genres/<%= genre.slug %>"><%= genre.name %></a>
			</li>
		<%end%>
	</ul>
		

	
</body>
</html>